package p599;

import java.util.Scanner;

//1988-Seoul-Olympic!!!
public class Main {
	public static void main(String[] args) {
		Scanner sc = new Scanner(System.in);
		String inp = sc.next();
		String inp2 = inp.toUpperCase();
		sc.close();

		for (int i = 0; i < inp2.length(); i++) {
			char c = inp2.charAt(i);
			if (Character.isAlphabetic(c)) {
				System.out.print(c);
			}
		}
	}
}
